num1 = input("Primeiro numero: ")
num2 = input("Segundo numero: ")
soma = int(num1) + int(num2)
print('A soma é =', soma)

print("")

diferença = int(num1) - int(num2)
print("A diferença é =", diferença)

print("")

multiplicar = int(num1) * int(num2)
print("A multiplicação é =", multiplicar)

print("")

divisão = int(num1) / int(num2)
print("A divisao é =", divisão)
